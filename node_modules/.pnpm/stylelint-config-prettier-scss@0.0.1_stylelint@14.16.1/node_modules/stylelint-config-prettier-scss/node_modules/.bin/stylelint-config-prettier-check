#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/suminhan/Documents/project/minna's projects/LandDesign/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/suminhan/Documents/project/minna's projects/LandDesign/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../stylelint-config-prettier@9.0.5_stylelint@14.16.1/node_modules/stylelint-config-prettier/bin/check.js" "$@"
else
  exec node  "$basedir/../../../../../stylelint-config-prettier@9.0.5_stylelint@14.16.1/node_modules/stylelint-config-prettier/bin/check.js" "$@"
fi
